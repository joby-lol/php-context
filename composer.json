{
    "name": "joby/context",
    "description": "A global static dependency injection container, because sometimes the old way is the good way ... if you mix it with the new way.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "~8.2"
    },
    "autoload": {
        "psr-4": {
            "Joby\\ContextInjection\\": "src/"
        },
        "files": [
            "functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Joby\\ContextInjection\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Joby Elliott",
            "email": "code@joby.lol"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^12.1",
        "phpstan/phpstan": "^2.1"
    }
}
